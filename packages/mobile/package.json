    "babel-preset-react": "6.24.1",
    "file-loader": "2.0.0",
    "gh-pages": "1.2.0",
    "react-docgen-typescript": "1.7.0",
    "react-styleguidist": "7.3.1",
    "ts-loader": "4.5.0",
    "tslint": "5.11.0",
