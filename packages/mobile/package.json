{
  "name": "@qiwi/pijma-mobile",
  "version": "1.139.0",
  "main": "lib/es6/index.js",
  "types": "lib/es6/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "clean": "rm -rf lib styleguide/lib",
    "build": "../../node_modules/.bin/concurrently 'npm:build:*'",
    "build:guide": "NODE_ENV=production styleguidist build",
    "build:es5": "node ../infra/esbuild.js",
    "build:libdef": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "watch": "../../node_modules/.bin/styleguidist server",
    "lint": "../../node_modules/.bin/tslint -p tsconfig.build.json src/**/*.ts{,x}",
    "lint:fix": "../../node_modules/.bin/tslint -p tsconfig.build.json --fix src/**/*.ts{,x}",
    "guide": "../../node_modules/.bin/gh-pages -d styleguide/lib -e mobile -m \"update mobile style guide v$npm_package_version\"",
    "guide_ci": "../../node_modules/.bin/gh-pages -d styleguide/lib -e mobile -m \"update mobile style guide v$npm_package_version\" -r \"https://$GH_TOKEN@github.com/qiwi/pijma.git\""
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "lib"
  ],
  "dependencies": {
    "@qiwi/pijma-core": "1.141.0",
    "@qiwi/pijma-fonts": "1.0.0"
  },
  "devDependencies": {
    "@qiwi/pijma-infra": "1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/pijma.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/pijma/issues"
  },
  "homepage": "https://github.com/qiwi/pijma/#readme"
}
