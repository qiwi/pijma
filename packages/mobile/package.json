  "scripts": {
    "clean": "rm -rf lib styleguide/lib",
    "build": "mkdir -p lib/es5 && tsc -p tsconfig.build.json && find lib -name *.d.ts -exec sed -i \"/\\b\\(reference path\\)\\b/d\" {} \\; && BABEL_ENV=production babel -q lib/es5 --out-dir lib/es5 && styleguidist build",
    "watch": "styleguidist server",
    "guide": "gh-pages -d styleguide/lib -e mobile -m \"update mobile style guide v$npm_package_version\""
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "lib/"
  ],
  "dependencies": {
    "@qiwi/pijma-core": "^1.0.0",
