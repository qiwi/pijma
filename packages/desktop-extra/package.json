{
  "name": "@qiwi/pijma-desktop-extra",
  "version": "0.0.0",
  "type": "module",
  "main": "./lib/es5/index.js",
  "module": "./lib/es6/index.js",
  "types": "./lib/dts/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/dts/index.d.ts",
      "import": "./lib/es6/index.js",
      "require": "./lib/es5/index.js"
    }
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf lib styleguide/lib node_modules",
    "build": "concurrently 'npm:build:*' 'npm:guide:build'",
    "guide:build": "exit 0",
    "guide:start": "exit 0",
    "static:start": "exit 0",
    "chrome:start": "exit 0",
    "build:es5": "swc src -d lib/es5 --config-file swc.es5.json && tsc-esm-fix --target=lib/es5 --fillBlank",
    "build:es6": "swc src -d lib/es6 --config-file swc.es6.json && tsc-esm-fix --target=lib/es6 --fillBlank",
    "build:dts": "tsc --project tsconfig.dts.json",
    "test:start": "jest",
    "lint": "eslint --fix src && prettier --loglevel silent --write src"
  },
  "files": [
    "README.md",
    "lib"
  ],
  "dependencies": {
    "@qiwi/common-formatters": "^1.2.1",
    "@qiwi/pijma-core": "workspace:*",
    "@qiwi/pijma-desktop": "workspace:*",
    "lodash-es": "^4.17.21",
    "react-day-picker": "^8.1.0",
    "react-dropzone": "^14.2.2",
    "react-i18next": "^11.18.4",
    "react-js-pagination": "^3.0.3",
    "react-table": "^7.8.0"
  },
  "devDependencies": {
    "@qiwi/pijma-infra": "workspace:*",
    "@swc/core": "^1.2.233",
    "@swc/jest": "^0.2.22",
    "@types/jest": "^28.1.7",
    "@types/lodash-es": "^4",
    "@types/react-js-pagination": "^3",
    "@types/react-table": "^7.7.12",
    "@types/react-test-renderer": "^18",
    "concurrently": "7.3.0",
    "css-loader": "6.7.1",
    "eslint": "8.21.0",
    "eslint-config-qiwi": "1.17.4",
    "fast-glob": "3.2.11",
    "file-loader": "6.2.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "2.7.1",
    "prettier-config-qiwi": "1.7.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-test-renderer": "^18.2.0",
    "style-loader": "3.3.1",
    "tsc-esm-fix": "2.20.0",
    "typescript": "4.7.4",
    "wait-on": "6.0.1",
    "webpack": "4.46.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/pijma.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/pijma/issues"
  },
  "homepage": "https://github.com/qiwi/pijma/#readme"
}
