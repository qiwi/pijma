{
  "name": "@qiwi/pijma-core",
  "version": "1.3.0",
  "main": "lib/es5/index.js",
  "types": "lib/es5/index.d.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "BABEL_ENV=production mkdir -p lib/es5 && cp -r src/fonts lib/es5/fonts && tsc -p tsconfig.build.json && babel -q lib/es5/ --out-dir lib/es5",
    "watch": "BABEL_ENV=development mkdir -p lib/es5 && cp -r src/fonts lib/es5/fonts && tsc -p tsconfig.build.json -w"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "lib/"
  ],
  "dependencies": {
    "emotion": "^9.2.6",
    "emotion-theming": "^9.2.3",
    "polished": "^1.9.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-emotion": "^9.2.3",
    "tslib": "^1.9.2"
  },
  "devDependencies": {
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-esnext": "^1.1.3",
    "babel-preset-react": "^6.24.1",
    "babel-plugin-emotion": "^9.2.6",
    "tslint": "^5.10.0",
    "typescript": "^2.8.4"
  }
}
