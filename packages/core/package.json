{
  "name": "@qiwi/pijma-core",
  "version": "0.0.0",
  "type": "module",
  "main": "./lib/es5/index.js",
  "module": "./lib/es6/index.js",
  "types": "./lib/dts/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/dts/index.d.ts",
      "import": "./lib/es6/index.js",
      "require": "./lib/es5/index.js"
    }
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf lib node_modules",
    "build": "concurrently 'npm:build:*'",
    "build:es5": "swc src -d lib/es5 --config-file swc.es5.json && tsc-esm-fix --target=lib/es5 --fillBlank",
    "build:es6": "swc src -d lib/es6 --config-file swc.es6.json && tsc-esm-fix --target=lib/es6 --fillBlank",
    "build:dts": "tsc --project tsconfig.dts.json",
    "lint": "eslint --fix src && prettier --loglevel silent --write src"
  },
  "files": [
    "README.md",
    "lib"
  ],
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@swc/helpers": "^0.4.3",
    "@types/dom-helpers": "^5.0.1",
    "@types/react-google-recaptcha": "^2.1.5",
    "@types/react-overlays": "^3.1.0",
    "@types/react-transition-group": "^4.4.5",
    "dom-helpers": "^5.2.1",
    "markdown-to-jsx": "^7.1.7",
    "react-google-recaptcha": "^2.1.0",
    "react-intersection-observer": "^9.4.0",
    "react-overlays": "^5.2.0",
    "react-transition-group": "^4.4.5",
    "text-mask-core": "^5.1.2"
  },
  "peerDependencies": {
    "@types/react": ">=16.14.0",
    "@types/react-dom": ">=16.9.0",
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "devDependencies": {
    "@types/react": "18.0.16",
    "@types/react-dom": "18.0.6",
    "concurrently": "7.3.0",
    "css-loader": "6.7.1",
    "eslint": "8.21.0",
    "eslint-config-qiwi": "1.17.4",
    "fast-glob": "3.2.11",
    "file-loader": "6.2.0",
    "prettier": "2.7.1",
    "prettier-config-qiwi": "1.7.2",
    "react": "18.2.0",
    "react-docgen-typescript": "patch:react-docgen-typescript@npm%3A2.2.2#~/.yarn/patches/react-docgen-typescript-npm-2.2.2-afb9698a32.patch",
    "react-dom": "18.2.0",
    "react-styleguidist": "11.2.0",
    "style-loader": "3.3.1",
    "tsc-esm-fix": "2.20.0",
    "typescript": "4.7.4",
    "webpack": "4.46.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/pijma.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/pijma/issues"
  },
  "homepage": "https://github.com/qiwi/pijma/#readme"
}
