{
  "name": "@qiwi/pijma-core",
  "version": "1.149.3",
  "main": "./lib/es5/index.js",
  "exports": {
    ".": {
      "import": "./lib/es6/index.js",
      "require": "./lib/es5/index.js"
    }
  },
  "type": "module",
  "types": "./lib/es6/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "concurrently 'npm:build:*'",
    "build:es6": "node ../infra/esbuild.js && tsc-esm-fix --target=lib/es6 --fillBlank",
    "build:es5": "tsc -p tsconfig.build.json --target es5 --outDir lib/es5 && tsc-esm-fix --target=lib/es5 --fillBlank",
    "build:libdef": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "lint": "eslint --fix src && prettier --loglevel silent --write src"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "lib"
  ],
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/css": "^11.9.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@types/dom-helpers": "^3.4.1",
    "@types/react": ">=16.8.23 <17.1.0",
    "@types/react-dom": ">=16.8.4 <17.1.0",
    "@types/react-google-recaptcha": "^2.1.5",
    "@types/react-overlays": "^1.1.3",
    "@types/react-transition-group": "^4.4.4",
    "dom-helpers": "^3.4.0",
    "markdown-to-jsx": "^7.1.7",
    "prettier": "2.7.1",
    "prettier-config-qiwi": "1.7.1",
    "react": ">=16.8.6 <17.1.0",
    "react-dom": ">=16.8.6 <17.1.0",
    "react-google-recaptcha": "^2.1.0",
    "react-intersection-observer": "^8.24.1",
    "react-overlays": "^1.2.0",
    "react-transition-group": "^4.2.2",
    "react-waypoint": "^9.0.2",
    "text-mask-core": "^5.1.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "concurrently": "7.2.2",
    "css-loader": "4.3.0",
    "esbuild": "0.14.47",
    "esbuild-loader": "2.19.0",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "8.18.0",
    "eslint-config-qiwi": "1.17.1",
    "fast-glob": "3.2.11",
    "file-loader": "6.2.0",
    "husky": "8.0.1",
    "minimist": "1.2.6",
    "react-docgen-typescript": "2.2.2",
    "react-styleguidist": "11.2.0",
    "style-loader": "2.0.0",
    "ts-patch": "2.0.1",
    "ts-react-display-name": "1.2.2",
    "tsc-esm-fix": "2.18.0",
    "typescript": "4.4.4",
    "webpack": "4.46.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/pijma.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/pijma/issues"
  },
  "homepage": "https://github.com/qiwi/pijma/#readme"
}
