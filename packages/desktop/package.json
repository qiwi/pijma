{
  "name": "@qiwi/pijma-desktop",
  "version": "1.127.2",
  "main": "lib/es5/index.js",
  "types": "lib/es5/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf lib styleguide/lib",
    "build": "mkdir -p lib/es5 && ttsc -p tsconfig.build.json && styleguidist build",
    "watch": "styleguidist server",
    "lint": "tslint -p tsconfig.build.json src/**/*.ts{,x}",
    "lint:fix": "tslint -p tsconfig.build.json --fix src/**/*.ts{,x}",
    "guide": "gh-pages -d styleguide/lib -e desktop -m \"update desktop style guide v$npm_package_version\"",
    "guide_ci": "gh-pages -d styleguide/lib -e desktop -m \"update desktop style guide v$npm_package_version\" -r \"https://$GH_TOKEN@github.com/qiwi/pijma.git\""
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "lib"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.16",
    "@emotion/styled": "^10.0.15",
    "@qiwi/pijma-core": "1.127.1",
    "@types/markdown-to-jsx": "^6.9.0",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-overlays": "^1.1.3",
    "@types/react-text-mask": "^5.4.6",
    "@types/react-transition-group": "^4.2.2",
    "emotion": "^10.0.14",
    "emotion-theming": "^10.0.14",
    "markdown-to-jsx": "^6.10.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-overlays": "^1.2.0",
    "react-text-mask": "^5.4.3",
    "react-transition-group": "^4.2.2",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@qiwi/pijma-infra": "1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/pijma.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/pijma/issues"
  },
  "homepage": "https://github.com/qiwi/pijma/#readme"
}
